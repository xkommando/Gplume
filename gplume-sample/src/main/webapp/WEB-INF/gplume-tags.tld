<?xml version="1.0" encoding="UTF-8"?><taglib xmlns="http://java.sun.com/xml/ns/j2ee"        version="2.0">	<tlib-version>1.0</tlib-version>	<short-name>gplume-tags</short-name>	<uri>com.caibowen.gplume.taglib</uri>	<tag>		<name>msg</name>		<tag-class>com.caibowen.gplume.misc.test.jsp.tag.TagMessage</tag-class>		<body-content>empty</body-content>		<attribute>			<name>k</name>			<required>true</required>			<rtexprvalue>false</rtexprvalue>		</attribute>	</tag>		<tag>		<name>link</name>		<tag-class>com.caibowen.gplume.misc.test.jsp.tag.TagLink</tag-class>		<body-content>empty</body-content>		<attribute>			<name>nativeName</name>			<required>false</required>			<rtexprvalue>false</rtexprvalue>		</attribute>		<attribute>			<name>displayName</name>			<required>false</required>			<rtexprvalue>false</rtexprvalue>		</attribute>		<attribute>			<name>url</name>			<required>true</required>			<rtexprvalue>true</rtexprvalue>		</attribute>		<attribute>			<name>target</name>			<required>false</required>			<rtexprvalue>false</rtexprvalue>		</attribute>	</tag>	</taglib>